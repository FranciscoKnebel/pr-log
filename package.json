{
    "name": "pr-log",
    "version": "1.0.0",
    "description": "Changelog generator based on GitHub Pull Requests",
    "bin": "bin/pr-log.js",
    "scripts": {
        "pretest": "eslint .",
        "test": "npm run test:unit --coverage",
        "posttest": "npm run check-coverage",
        "test:unit": "istanbul test _mocha -- test",
        "check-coverage": "istanbul check-coverage --statement 100 --branch 100 --function 100 --lines 100",
        "coveralls": "cat ./build/coverage/lcov.info | coveralls"
    },
    "author": "Mathias Schreck <schreck.mathias@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "bluebird": "2.6.2",
        "commander": "2.6.0",
        "git-promise": "0.2.0",
        "lodash.find": "2.4.1",
        "lodash.groupby": "2.4.1",
        "moment": "2.8.4",
        "node-url-from-git": "1.0.0",
        "normalize-git-url": "1.0.0",
        "parse-github-repo-url": "1.0.0",
        "prepend": "1.0.2",
        "semver": "4.2.0",
        "superagent-promise": "0.2.0"
    },
    "devDependencies": {
        "chai": "1.10.0",
        "chai-as-promised": "4.1.1",
        "coveralls": "2.11.2",
        "eslint": "0.11.0",
        "eslint-plugin-mocha": "0.2.2",
        "istanbul": "0.3.5",
        "mocha": "2.1.0",
        "proxyquire": "1.3.0",
        "sinon": "1.12.2",
        "sinon-as-promised": "2.0.3",
        "sinon-chai": "2.6.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lo1tuma/pr-log.git"
    },
    "keywords": [
        "pr-log",
        "changelog",
        "changelog.md",
        "github",
        "history",
        "history.md"
    ],
    "contributors": [
        "Alexander Schmidt <alexanderschmidt1@gmail.com>"
    ]
}
